# Production Environment Variables for Railway
# Copy these to your Railway project environment variables

# MongoDB Connection
MONGODB_URI=your_mongodb_connection_string_from_railway

# Server Configuration  
PORT=5000
NODE_ENV=production

# GitHub API Integration (REQUIRED for auto-deployment)
GITHUB_TOKEN=ghp_your_personal_access_token_here
GITHUB_USERNAME=MRKrog
GITHUB_REPO=ai-experiment

# Production Auto-Deployment Settings
ENABLE_AUTO_DEPLOYMENT=false
ENABLE_PRODUCTION_DEPLOY=true

# Note: REACT_PROJECT_PATH not needed in production
# Components are deployed directly to GitHub via API 